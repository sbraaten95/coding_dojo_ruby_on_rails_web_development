<h1>Listing users</h1>
<table>
	<thead>
		<tr>
			<th>
				First name
			</th>
			<th>
				Last name
			</th>
			<th>
				Email
			</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |u| %>
		<tr>
			<td>
				<%= u[:first_name] %>
			</td>
			<td>
				<%= u[:last_name] %>
			</td>
			<td>
				<%= u[:email_address] %>
			</td>
			<td>
				<a href="users/<%=u.id%>">Show</a>
				<a href="users/<%=u.id%>/edit">Edit</a>
				<a href="users/<%=u.id%>" data-method="delete">Delete</a>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>
<a href="/users/new"><button>New User</button></a>